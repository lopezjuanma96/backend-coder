//linting setup file, requires some imports, see package.json devDependencies
{
    "extends": "airbnb-base", //airbnb code styling
    "rules": {
      "no-console": "off",
      "max-len": [
        "error",
        {
          "code": 120,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreComments": true
        }
      ],
      "newline-before-return": "error",
      "implicit-arrow-linebreak": "off"
    }
  }
  